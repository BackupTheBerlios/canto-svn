<hibernate-mapping package="li.rajenlab.canto.core.domain.job">

  <class name="JobLevel" table="JOB_LEVEL">
  
	<!-- Entity mapping -->
    <id name="id" column="ID" type="long">
      <generator class="native"/>
    </id>
    <property name="creationTime" type="timestamp">
      <column name="CREATION_TIME"/>
    </property>
    <property name="changeTime" type="timestamp">
      <column name="CHANGE_TIME"/>
    </property>
    
    <!-- Property mapping -->
    <property name="level" length="100">
    	<column name="LEVEL"/>
    </property>
    <property name="sortOrder">
        <column name="SORT_ORDER" not-null="true" unique="true"/>
    </property>

  </class>
    
</hibernate-mapping>   
