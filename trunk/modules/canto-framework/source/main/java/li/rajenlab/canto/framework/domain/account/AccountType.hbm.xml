<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="li.rajenlab.canto.common.domain.account">

  <class name="AccountType" table="CANTO_ACCOUNT_TYPE">
  
	<!-- Entity mapping -->
    <id name="id" column="ID" type="long">
      <generator class="native"/>
    </id>
    <property name="creationTime" type="timestamp">
      <column name="CREATION_TIME"/>
    </property>
    <property name="changeTime" type="timestamp">
      <column name="CHANGE_TIME"/>
    </property>
    
    <!-- Property mapping -->
    <property name="name">
    	<column name="ACCOUNT_TYPE" unique="true" not-null="true"/>
    </property>
  
  </class>
    
</hibernate-mapping>   
